{
  "SettingsMongoDatabase": {
    "StringConnection": "mongodb+srv://<user>:<pass>@<cluster>",
    "DatabaseName": "Teleperformance",
    "PersonCollectionName": "People",
    "ProductCollectionName": "Products",
    "CartCollectionName": "Carts"
  },
  "EncryptionSettings": {
    "Iterations": 1000,
    "DesiredKeyLength": 16,
    "Salt": "SecureTeleperformanceSalt"
  },
  "JwtSettings": {
    "SecretKey": "MySecretKeyToken",
    "Issuer": "localhost",
    "Audience": "localhost"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
